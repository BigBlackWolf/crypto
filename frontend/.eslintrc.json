{
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "babel-eslint",
      "ecmaVersion": 8,
      "allowImportExportEverywhere": true,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
    },
    "rules": {
        "vue/max-attributes-per-line": "off",
        "vue/attributes-order": "off",
        "no-console": "off",
        "vue/html-self-closing": "off"
    },
    "extends": [
        "plugin:vue/recommended"
    ]
}

